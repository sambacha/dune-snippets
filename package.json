  "files": [
    "/dune/**/*",
    "prices/**/*",
    "schemas/**/*"
  ],
  "scripts": {
    "build:index": "rm dune/index.json &&  tree -Jf dune/ > dune/index.json",
    "release": "npx semantic-release",
