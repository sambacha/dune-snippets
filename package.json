    "ethereum",
    "dune analytics",
    "defi",
    "big query",
    "kpi",
    "sql",
    "data",
    "analytics"
  ],
  "author": "sam bacha <sam@freighttrust.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sambacha/dune-snippets/issues"
  },
  "homepage": "https://github.com/sambacha/dune-snippets#readme",
  "devDependencies": {
    "prettier": "2.2.1",
    "clang-format": "*"
  }
}
